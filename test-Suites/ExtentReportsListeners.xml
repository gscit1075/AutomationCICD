<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

    <!-- <suite parallel="tests"  name="Suite"> when run the all test class parallely-->
    
 <suite name="Suite" parallel="tests">

		<listeners>
		
		    <listener class-name="rahulshettyacademy.TestComponents.Listeners"/>
				
		</listeners>	
		

 <test parallel="methods" thread-count="10" name="Submit Order Test">
    <classes>
       <class name="rahulshettyacademy.Tests.Class2B_SubmitOrderTest"/>
    </classes>
  </test> <!-- Test -->
  
   <test thread-count="5" name="Error Validation Test">
    <classes>
     <class name="rahulshettyacademy.Tests.Class2B_ErrorValidaionTest"/>
    </classes>
  </test> <!-- Test -->
    
  
 
</suite> <!-- Suite -->
